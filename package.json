{
  "name": "event-scheduler",
  "version": "0.0.1",
  "description": "A node.js server that let's you schedule a cron event. This will call a configured webhook. Event's can be repeating or a one time only thing",
  "main": "/dist/index.js",
  "scripts": {
    "prestart": "yarn run build",
    "build": "tsc",
    "start": "node ./dist/index.js",
    "watch": "nodemon ./src/index.ts"
  },
  "keywords": [
    "event",
    "cron",
    "webhook"
  ],
  "author": "KyleMoran138",
  "license": "ISC",
  "dependencies": {
    "@types/cron": "^1.7.3",
    "@types/express": "^4.17.13",
    "@types/node-cron": "^3.0.1",
    "cron": "^1.8.2",
    "dotenv": "^14.2.0",
    "express": "^4.17.2",
    "firebase": "^9.6.3",
    "firebase-admin": "^10.0.1",
    "joi": "^17.6.0",
    "mongoose": "^6.1.0",
    "node-cron": "^3.0.0",
    "nodemon": "^2.0.15",
    "ts-node": "^10.4.0"
  },
  "devDependencies": {
    "typescript": "^4.5.5"
  }
}
